@{
}

<h1>Coding Events</h1>

<table class="table-responsive-xl table-striped table-bordered">
    <tr>
        <td>Event Name</td>
        <td>Description</td>
    </tr>
    
    @foreach(KeyValuePair<string, string>meeting in ViewBag.events)
    {
        <tr>
            <td>@meeting.Key</td>
            <td>@meeting.Value</td>
        </tr>;
    }
</table>

<a asp-controller="Events" asp-action="Add">Add Event</a>


